{
  "name": "kittik-shape-text",
  "version": "0.0.0-semantic-release",
  "description": "Text shape for Kittik",
  "main": "lib/Text.js",
  "author": "ghaiklor",
  "homepage": "https://github.com/kittikjs/shape-text#readme",
  "bugs": {
    "url": "https://github.com/kittikjs/shape-text/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kittikjs/shape-text.git"
  },
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf ./lib",
    "compile": "npm run clean; babel src --out-dir lib",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "prepublish": "npm run compile",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "test": "babel-node ./node_modules/.bin/isparta cover _mocha"
  },
  "dependencies": {
    "kittik-shape-basic": "2.1.2"
  },
  "devDependencies": {
    "babel-cli": "6.4.5",
    "babel-preset-es2015-node4": "2.0.3",
    "chai": "3.4.1",
    "coveralls": "2.11.6",
    "cz-conventional-changelog": "1.1.5",
    "isparta": "4.0.0",
    "kittik-cursor": "2.4.0",
    "mocha": "2.4.2",
    "semantic-release": "4.3.5",
    "sinon": "1.17.2"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "tag": "latest"
  },
  "release": {
    "branch": "master"
  }
}
